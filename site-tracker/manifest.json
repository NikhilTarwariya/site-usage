{
  "manifest_version": 3,
  "name": "Usage Tracker",
  "version": "1.0",
  "description": "Track time spent on websites",
  "permissions": ["tabs", "storage", "alarms"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
